{
  "name": "raspberry_pi",
  "description": "managing the raspberry pi devices",
  "json_class": "Chef::Role",
  "default_attributes": {
    "install_adds": {
      "authorization": {
        "sudo": {
          "groups": [
            "wheel",
            "adm"
          ],
          "passwordless": "true"
        }
      },
      "gem_set": "rails",
    },
    "ssh_config": {
      "alt_port": "2222",
      "enable_scp": true,
      "key_size": "4096",
      "keys_set": true,
      "log_level": "INFO",
      "port": "22",
      "root_login": "without-password",
      "server_key_size": "1024",
      "use_dsa": false,
      "use_ed25519": false,
      "use_rsa": true
    }
  },
  "override_attributes": {

  },
  "chef_type": "role",
  "run_list": [
  "recipe[hello_chef_server]",
  "recipe[quinncloud::initial_setup]",
  "recipe[motd::default]"
]
,
  "env_run_lists": {

  }
}
